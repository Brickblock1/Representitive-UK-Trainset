spriteset(2aspect_g_en, "gfx/signal_electric_2asp.png") {template_signals(40,85)}
spriteset(2aspect_r_ex, "gfx/signal_electric_2asp.png") {template_signals(10,120)}
spriteset(2aspect_g_ex, "gfx/signal_electric_2asp.png") {template_signals(40,120)}
spriteset(2aspect_r_co, "gfx/signal_electric_2asp.png") {template_signals(10,155)}
