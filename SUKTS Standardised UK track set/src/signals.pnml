//Switches for 2 aspect signals

switch(FEAT_RAILTYPES, SELF, sw_2aspect_r, getbits(extra_callback_info2, 16, 8)){
00 : 2aspect_r_block;
01 : 2aspect_r_en;
02 : 2aspect_r_ex;
03 : 2aspect_r_co;
04 : 2aspect_r_path;
05 : 2aspect_r_owp;
}

switch(FEAT_RAILTYPES, SELF, sw_2aspect_g, getbits(extra_callback_info2, 16, 8)){
00 : 2aspect_g_block;
01 : 2aspect_g_en;
02 : 2aspect_g_ex;
03 : 2aspect_g_co;
04 : 2aspect_g_path;
05 : 2aspect_g_owp;
}

switch(FEAT_RAILTYPES, SELF, sw_2aspect, getbits(extra_callback_info2, 0, 8)){
00 : sw_2aspect_r;
01 : sw_2aspect_g;
}

//What kind of electric signal do you want?

switch(FEAT_RAILTYPES, SELF, sw_electric_signals, signal_elec){
0 : sw_2aspect;
sw_2aspect;
}

switch(FEAT_RAILTYPES, SELF, sw_signals, getbits(extra_callback_info2, 8, 8)){
0 : sw_electric_signals;
sw_2aspect;
}